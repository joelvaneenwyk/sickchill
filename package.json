{
  "name": "sickchill",
  "version": "4.0.72",
  "private": true,
  "packageManager": "yarn@4.0.2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joelvaneenwyk/SickChill.git"
  },
  "bugs": {
    "url": "https://github.com/joelvaneenwyk/SickChill/issues"
  },
  "workspaces": [
    "contrib/setup-browser-env"
  ],
  "homepage": "https://github.com/joelvaneenwyk/SickChill#readme",
  "scripts": {
    "xo": "echo \"Executing 'xo' command...\" && xo --verbose --fix \"**/*\"",
    "xo:fix": "yarn xo -- --fix",
    "ava": "ava --verbose",
    "test": "yarn test:xo && yarn test:ava",
    "test:xo": "yarn xo && echo \"[xo] Done.\"",
    "test:ava": "yarn ava && echo \"[ava] Done.\"",
    "security": "snyk test --all-projects",
    "security:fix": "snyk fix --all-projects",
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "serve": "webpack serve"
  },
  "browser-env": {
    "globals": [
      "window",
      "document"
    ],
    "jsdom": {
      "userAgent": "SickChillUserAgent"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.24.3",
    "@babel/register": "^7.23.7",
    "@eslint-react/eslint-plugin": "^0.10.2",
    "@popperjs/core": "^2.11.8",
    "@webpack-cli/generators": "^3.0.7",
    "@yarnpkg/cli": "^4.0.2",
    "@yarnpkg/core": "^4.0.2",
    "@yarnpkg/plugin-git": "^3.0.0",
    "@yarnpkg/plugin-workspace-tools": "^4.0.1",
    "ava": "^6.0.1",
    "babel-loader": "^9.1.3",
    "bootstrap": "^5.3.2",
    "bower": "^1.8.14",
    "browser-env": "^3.3.0",
    "commitlint": "^18.4.4",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": ">=8.56.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-react": "^0.27.0",
    "eslint-import-resolver-webpack": "^0.13.8",
    "eslint-plugin-ava": "^14.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unicorn": "^50.0.1",
    "esm": "^3.2.25",
    "grunt": "^1.6.1",
    "grunt-bower-concat": "git+https://github.com/joelvaneenwyk/grunt-bower-concat.git",
    "grunt-bower-task": "^0.6.2",
    "grunt-cli": "^1.4.3",
    "grunt-contrib-clean": "^2.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^5.0.0",
    "grunt-contrib-jshint": "^3.2.0",
