{
  "[ignore]": {
    "editor.defaultFormatter": "lkrms.inifmt"
  },
  "[ini]": {
    "editor.defaultFormatter": "lkrms.inifmt"
  },
  "[shellscript]": {
    "files.eol": "\n",
    "editor.codeActionsOnSave": {
      "source.fixAll.shellcheck": "explicit"
    },
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[javascript]": {
    "editor.defaultFormatter": "samverschueren.linter-xo"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "samverschueren.linter-xo"
  },
  "[typescript]": {
    "editor.defaultFormatter": "samverschueren.linter-xo"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "samverschueren.linter-xo"
  },
  "application.experimental.rendererProfiling": true,
  "ava-runner.experimentalEnabled": true,
  "cSpell.words": [
    "buildx",
    "sickchill"
  ],
  "dotnetAcquisitionExtension.installTimeoutValue": 6000,
  "editor.formatOnSave": true,
  "eslint.enable": false,
  "eslint.experimental.useFlatConfig": false,
  "eslint.format.enable": false,
  "eslint.lintTask.enable": false,
  "files.associations": {
    "*.ini": "ini",
    ".mypy.ini": "ini",
    "MLTable": "yaml"
  },
  "python.createEnvironment.contentButton": "show",
  "python.createEnvironment.trigger": "off",
  "python.languageServer": "Pylance",
  "python.missingPackage.severity": "Error",
  "python.terminal.activateEnvInCurrentTerminal": true,
  "python.terminal.focusAfterLaunch": true,
  "python.testing.autoTestDiscoverOnSaveEnabled": true,
  "python.testing.pytestArgs": [
    "tests"
  ],
  "python.testing.pytestEnabled": true,
  "python.testing.unittestEnabled": false,
  "xo.enable": true,
  "xo.format.enable": true,
  "xo.options": {
    "overrideConfigFile": "xo.config.cjs"
  },
  "xo.overrideSeverity": "error",
  "xo.statusBar": "Always",
  "xo.trace.server": "messages",
  "xo.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ]
}
